{
  "manifest_version": 3,
  "name": "Parental Control",
  "version": "1.0",
  "description": "Parental Control extension - by Ping.",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "windows",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "background": {
    "service_worker": "extension/out/parental_control_background.bundle.js",
    "type": "module"
  },
  "action": {
    "default_popup": "/content/ui/popup.tsx",
    "default_position": "center",
    "default_icon": {
      "16": "/assets/Logo.png",
      "48": "/assets/Logo.png",
      "128": "/assets/Logo.png"
    }
  },

  "icons": {
    "16": "/assets/Logo.png",
    "48": "/assets/Logo.png",
    "128": "/assets/Logo.png"
  },
  "key": ""
}
